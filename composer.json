{
    "name": "zitec/js-data-bundle",
    "description": "This bundle transports pieces of information from PHP to JS.",
    "homepage": "https://www.zitec.com",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "ext-json": "*",
        "php": ">=7.4.0",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "symfony/property-access": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/event-dispatcher": "^4.4 || ^5.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    },
    "authors": [
        {
            "name": "Zitec Team",
            "homepage": "https://www.zitec.com"
        }
    ],
    "config": {
        "discard-changes": true,
        "platform": {
            "php": "7.4.0"
        }
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Zitec\\JSDataBundle\\": ""
        }
    }
}
